		<query language="SQL"><![CDATA[SELECT
    bh.invoiceno,
    bh.recid AS header_id,
    bh.invoicetoaccountno AS accountnumber,
    bh.invoicetoname AS customername,
    bh.billaddress,
    bh.billunit,
    bh.billpostal,
    bh.billprov,
    bh.billcity,
    bh.contactname,
    bh.contactphone,
    bh.contmail AS contactemail,
    bh.createddate AS invoice_createddate,
    bh.min_fromdate,
    bh.max_fromdate,
    bh.total,
    bh.taxsum,
    bh.installationsum,
    bh.valuesum,
    bh.prev_collectionvalue * -1 AS paid,
    bh.collectionstatusid,
    bh.serializeddate,
    COALESCE(bh.branchid, 0) AS branchid,
    bh.area,
    COALESCE(bh.headertype, '') AS headertype,
    bh.bill_grandtotal,
    bh.bill_pervpayment,
    bh.bill_currentpayment,
    bh.bill_prevrundate,
    bh.bill_currentrundate,
    bh.invoicestartdate,
    COALESCE(bh.depositbalance, 0) AS depositbalance,

    bl.recid AS line_id,
    bl.orderserviceid,
    bl.invoicefrom,
    bl.invoiceto,
    bl.componentsname,
    bl.service AS service,
    COALESCE(bl.linetypeid, 0) AS linetypeid,
    COALESCE(bl.quantity, 0) AS quantity,
    COALESCE(bl.debitvalue, 0) AS debitvalue,
    COALESCE(bl.creditvalue, 0) AS creditvalue,
    COALESCE(bl.debitvalue, 0) - COALESCE(bl.creditvalue, 0) AS type_charge,
    COALESCE(bl.changerate, 0) AS changerate,
    bl.comments,
    COALESCE(bl.beforpro, 0) AS beforpro,
    COALESCE(bl.discountvalue, 0) AS discountvalue,
    bl.billingfrequency,
    bl.entereddate AS line_createddate,
    bl.parent_orderserviceid,

    -- Currency ID and mapped value
    bl.currencyid,
    CASE
        WHEN bl.currencyid = 1 THEN 'USD'
        WHEN bl.currencyid = 3 THEN 'LE'
        WHEN bl.currencyid = 2 THEN 'IQD'
        ELSE 'IQD'
    END AS currency_name,

    br.rundate,
    br.run_caption,
    br.run_cover_to_date,
    br.agentname,
    br.laststatus,
    br.laststatusdate

    FROM bill_billheader bh
    JOIN bill_bill_lines bl ON bl.headerid = bh.recid
    JOIN bill_run br ON bh.runid = br.recid
    WHERE bh.invoiceno = '1247'
    ORDER BY bl.entereddate]]></query>
